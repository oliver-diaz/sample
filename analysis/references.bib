@article{RN190,
   author = {Thorlund, K. and Devereaux, P. J. and Wetterslev, J. and Guyatt, G. and Ioannidis, J. P. and Thabane, L. and Gluud, L. L. and Als-Nielsen, B. and Gluud, C.},
   title = {Can trial sequential monitoring boundaries reduce spurious inferences from meta-analyses?},
   journal = {Int J Epidemiol},
