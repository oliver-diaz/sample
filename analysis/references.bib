@article{RN186,
   author = {Cameron, C. and Fireman, B. and Hutton, B. and Clifford, T. and Coyle, D. and Wells, G. and Dormuth, C. R. and Platt, R. and Toh, S.},
   title = {Network meta-analysis incorporating randomized controlled trials and non-randomized comparative cohort studies for assessing the safety and effectiveness of medical treatments: challenges and opportunities},
   journal = {Syst Rev},
   volume = {4},
   pages = {147},
   note = {Cameron, Chris
Fireman, Bruce
