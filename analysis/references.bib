   author = {Schmitz, S. and Maguire, A. and Morris, J. and Ruggeri, K. and Haller, E. and Kuhn, I. and Leahy, J. and Homer, N. and Khan, A. and Bowden, J. and Buchanan, V. and O'Dwyer, M. and Cook, G. and Walsh, C.},
