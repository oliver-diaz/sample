@article{RN115,
   author = {Carlson, J. J. and Guzauskas, G. F. and Chapman, R. H. and Synnott, P. G. and Liu, S. and Russo, E. T. and Pearson, S. D. and Brouwer, E. D. and Ollendorf, D. A.},
   title = {Cost-effectiveness of Drugs to Treat Relapsed/Refractory Multiple Myeloma in the United States},
