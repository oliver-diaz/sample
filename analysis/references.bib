@article{RN298,
   author = {Arikian, S. R. and Milentijevic, D. and Binder, G. and Gibson, C. J. and Hu, X. H. and Nagarwala, Y. and Hussein, M. and Corvino, F. A. and Surinach, A. and Usmani, S. Z.},
   title = {Patterns of total cost and economic consequences of progression for patients with newly diagnosed multiple myeloma},
   journal = {Curr Med Res Opin},
   volume = {31},
   number = {6},
   pages = {1105-15},
   note = {Arikian, Steven R
Milentijevic, Dejan
