@article{RN140,
   author = {Botta, C. and Ciliberto, D. and Rossi, M. and Staropoli, N. and Cuce, M. and Galeano, T. and Tagliaferri, P. and Tassone, P.},
   title = {Network meta-analysis of randomized trials in multiple myeloma: efficacy and safety in relapsed/refractory patients},
   journal = {Blood Adv},
