@article{RN142,
   author = {Dimopoulos, M. A. and Kaufman, J. L. and White, D. and Cook, G. and Rizzo, M. and Xu, Y. and Fahrbach, K. and Gaudig, M. and Slavcev, M. and Dearden, L. and Lam, A.},
   title = {A Comparison of the Efficacy of Immunomodulatory-containing Regimens in Relapsed/Refractory Multiple Myeloma: A Network Meta-analysis},
   journal = {Clin Lymphoma Myeloma Leuk},
   volume = {18},
   number = {3},
   pages = {163-173 e6},
   note = {Dimopoulos, Meletios Athanasios
Kaufman, Jonathan L
