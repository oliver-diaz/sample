@article{RN113,
   author = {Blommestein, H. M. and Franken, M. G. and Uyl-de Groot, C. A.},
   title = {A practical guide for using registry data to inform decisions about the cost effectiveness of new cancer drugs: lessons learned from the PHAROS registry},
