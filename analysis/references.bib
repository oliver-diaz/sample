@article{RN205,
   author = {Patel, K. K. and Giri, S. and Parker, T. L. and Bar, N. and Neparidze, N. and Huntington, S. F.},
   title = {Cost-Effectiveness of First-Line Versus Second-Line Use of Daratumumab in Older, Transplant-Ineligible Patients With Multiple Myeloma},
   journal = {J Clin Oncol},
   volume = {39},
